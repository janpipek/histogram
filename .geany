[editor]
line_wrapping=true
line_break_column=80
auto_continue_multiline=true

[file_prefs]
final_new_line=true
ensure_convert_new_lines=true
strip_trailing_spaces=true
replace_tabs=true

[indentation]
indent_width=4
indent_type=0
indent_hard_tab_width=8
detect_indent=false
detect_indent_width=false
indent_mode=2

[project]
name=histogram
base_path=.
description=
file_patterns=*.py;*.ini;*.rst;*.y*ml;.gitignore;.hgignore;

[long line marker]
long_line_behaviour=1
long_line_column=80

[prjorg]
source_patterns=*.c;*.C;*.cpp;*.cxx;*.c++;*.cc;*.m;*.py;
header_patterns=*.h;*.H;*.hpp;*.hxx;*.h++;*.hh;
ignored_dirs_patterns=.*;CVS;
ignored_file_patterns=*.o;*.obj;*.a;*.lib;*.so;*.dll;*.lo;*.la;*.class;*.jar;*.pyc;*.mo;*.gmo;*.po;
generate_tag_prefs=0
external_dirs=

[build-menu]
NF_01_LB=test module
NF_01_CM=$HOME/local/scripts/myscripts/cnose 
NF_01_WD=%p
NF_02_LB=Make _Object
NF_02_CM=make %e.o
NF_02_WD=
EX_00_LB=_Execute
EX_00_CM=python "%f"
EX_00_WD=
PythonFT_00_LB=_Test
PythonFT_00_CM=$HOME/local/scripts/myscripts/cnose histogram test
PythonFT_00_WD=%p
filetypes=Python;
PythonFT_01_LB=
PythonFT_01_CM=
PythonFT_01_WD=
